{% set formContent %}
  {% if content.form %}
    {% form_theme content.form 'form/form_layout.html.twig' %}
    {{ form(content.form) }}

    {% if app.request.get('send') == 'true' %}
      {{ view.form.entity.successText|raw }}
    {% endif %}
  {% endif %}
{% endset %}

{% include "includes/common/_page_container.html.twig" with {
  'content': formContent,
  'class': 'my-4'
} only %}
