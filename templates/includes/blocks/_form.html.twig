<twig:page-container class="my-4">
	<twig:block name="content">
    {% if content.form %}
      <div class="not-prose">
        {{ form(content.form) }}
      </div>

      {% if app.request.get('send') == 'true' %}
        {{ view.form.entity.successText|raw }}
      {% endif %}
    {% endif %}
  </twig:block>
</twig:page-container>



