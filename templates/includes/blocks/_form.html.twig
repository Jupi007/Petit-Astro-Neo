{% embed "includes/common/_page_container.html.twig" with {
  content: content,
  class: 'my-4',
} only %}
  {% block content %}
    {% if content.form %}
      <div class="not-prose">
        {{ form(content.form) }}
      </div>

      {% if app.request.get('send') == 'true' %}
        {{ view.form.entity.successText|raw }}
      {% endif %}
    {% endif %}
  {% endblock %}
{% endembed %}


