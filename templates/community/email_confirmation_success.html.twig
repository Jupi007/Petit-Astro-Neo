{% extends 'base_fullart.html.twig' %}

{% set title = 'app.community.profile.title'|trans %}

{% block content %}
  {% if success %}
    <twig:alert variant="success">
      {{ 'community.email_confirmation.message'|trans }}
    </twig:alert>
  {% else %}
    <twig:alert variant="error">
      {{ 'app.invalid_token_message'|trans }}
    </twig:alert>
  {% endif %}
{% endblock %}
