<?xml version="1.0" encoding="UTF-8"?>
<formats xmlns="http://schemas.sulu.io/media/formats"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.sulu.io/media/formats http://schemas.sulu.io/media/formats-1.1.xsd">

    <format key="avatar_small">
        <meta>
            <title lang="fr">Avatar</title>
        </meta>

        <scale x="64" y="64"/>
    </format>

    <format key="avatar_small@2x">
        <meta>
            <title lang="fr">Avatar hdpi</title>
        </meta>

        <scale x="128" y="128"/>
    </format>

    <format key="avatar_large">
        <meta>
            <title lang="fr">Avatar</title>
        </meta>

        <scale x="192" y="192"/>
    </format>

    <format key="avatar_large@2x">
        <meta>
            <title lang="fr">Avatar hdpi</title>
        </meta>

        <scale x="384" y="384"/>
    </format>

    <format key="figure">
        <meta>
            <title lang="fr">Figure</title>
        </meta>

        <scale x="1920"/>
    </format>

    <format key="figure@2x">
        <meta>
            <title lang="fr">Figure hdpi</title>
        </meta>

        <scale x="3840"/>
    </format>
</formats>
