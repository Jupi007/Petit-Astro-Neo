<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>publication_typos</key>

    <joins name="publication">
        <join>
            <entity-name>App\Entity\Publication</entity-name>
            <field-name>App\Entity\PublicationTypo.publication</field-name>
        </join>
    </joins>

    <properties>
        <property name="id" translation="sulu_admin.id">
            <field-name>id</field-name>
            <entity-name>App\Entity\PublicationTypo</entity-name>
        </property>

        <property name="description" visibility="always" searchability="yes" translation="sulu_admin.description">
            <field-name>description</field-name>
            <entity-name>App\Entity\PublicationTypo</entity-name>
        </property>

        <property name="publicationId" visibility="never" searchability="no">
            <field-name>id</field-name>
            <entity-name>App\Entity\Publication</entity-name>

            <joins ref="publication"/>
        </property>
    </properties>
</list>
